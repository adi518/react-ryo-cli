{
  "name": "react-build",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "bin": "./index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kenshoo/react-build"
  },
  "scripts": {
    "push": "yalc publish --push",
    "unpublish": "yalc installations clean %npm_package_name%",
    "watch": "watch \"npm run push\" -s ./ --ignoreDirectoryPattern /node_modules/",
    "inspect": "node ./index.js build --inspect",
    "lint": "eslint ./",
    "eslint": "./node_modules/.bin/eslint --init",
    "test": "jest"
  },
  "dependencies": {
    "@craco/craco": "^5.6.3",
    "babel-plugin-lodash": "^3.3.4",
    "chalk": "^3.0.0",
    "core-js": "^3.6.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "figlet": "^1.2.4",
    "gradient-string": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-styled-components": "^7.0.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "node-sass": "^4.13.1",
    "pascal-case": "^3.1.1",
    "react-scripts": "^3.3.0",
    "react-test-renderer": "^16.12.0",
    "regenerator-runtime": "^0.13.3",
    "source-map-explorer": "^2.2.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.0",
    "slash": "^3.0.0",
    "watch": "^1.0.2"
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-airbnb-base",
      "prettier"
    ],
    "rules": {
      "quotes": 0,
      "comma-dangle": 0,
      "arrow-parens": 0,
      "linebreak-style": 0,
      "no-param-reassign": 0
    },
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    }
  }
}
