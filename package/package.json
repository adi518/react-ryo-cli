{
  "name": "react-ryo-cli",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "bin": "./bin/cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/adi518/react-ryo-cli"
  },
  "watch": {
    "push": {
      "patterns": [
        "./**/*"
      ],
      "extensions": "*",
      "ignore": [
        "node_modules"
      ]
    }
  },
  "scripts": {
    "push": "yalc push",
    "watch": "npm-watch",
    "unpublish": "yalc installations clean %npm_package_name%",
    "inspect": "node ./index.js build --inspect",
    "lint": "eslint ./",
    "eslint": "./node_modules/.bin/eslint --init",
    "test": "jest"
  },
  "dependencies": {
    "@craco/craco": "^5.6.3",
    "babel-plugin-lodash": "^3.3.4",
    "chalk": "^3.0.0",
    "core-js": "^3.6.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "figlet": "^1.2.4",
    "gradient-string": "^1.2.0",
    "identity-obj-proxy": "^3.0.0",
    "inquirer": "^7.0.4",
    "jest-styled-components": "^7.0.0",
    "lodash": "^4.17.15",
    "minimist": "^1.2.0",
    "node-sass": "^4.13.1",
    "pascal-case": "^3.1.1",
    "react-dev-utils": "^10.2.0",
    "react-scripts": "^3.3.0",
    "react-test-renderer": "^16.12.0",
    "regenerator-runtime": "^0.13.3",
    "source-map-explorer": "^2.2.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "npm-watch": "^0.6.0",
    "slash": "^3.0.0"
  }
}
