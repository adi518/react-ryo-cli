{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-scripts-custom": "^0.1.0"
  },
  "scripts": {
    "start": "react-scripts-custom start",
    "build": "react-scripts-custom build",
    "build:development": "react-scripts-custom build:development",
    "build:stats": "react-scripts-custom build:stats",
    "test": "react-scripts-custom test",
    "test:watch": "react-scripts-custom test:watch",
    "test:update": "react-scripts-custom test:update",
    "test:production": "react-scripts-custom test:production",
    "eject": "react-scripts-custom eject",
    "inspect": "react-scripts-custom build --inspect",
    "inspect:output": "react-scripts-custom build --inspect > output.json",
    "unknown-script": "react-scripts-custom unknown-script"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:4000"
}
